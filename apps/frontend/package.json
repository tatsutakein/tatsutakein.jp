{
  "name": "@tatsutakeinjp/frontend",
  "version": "1.3.2",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun codegen && bun with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "bun with-env next dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
    "start": "bun with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "codegen": "graphql-codegen-esm --config codegen.ts",
    "codegen:watch": "graphql-codegen-esm --config codegen.ts --watch"
  },
  "dependencies": {
    "@t3-oss/env-nextjs": "0.12.0",
    "@tanstack/react-query": "5.71.10",
    "@tatsutakeinjp/api": "workspace:*",
    "@tatsutakeinjp/auth": "workspace:*",
    "@tatsutakeinjp/db": "workspace:*",
    "@tatsutakeinjp/ui": "workspace:*",
    "@tatsutakeinjp/validators": "workspace:*",
    "date-fns": "2.30.0",
    "date-fns-tz": "2.0.1",
    "geist": "1.3.1",
    "graphql": "16.10.0",
    "graphql-request": "7.1.2",
    "next": "15.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sharp": "0.34.1",
    "superjson": "2.2.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/client-preset": "4.8.0",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-typed-document-node/core": "3.2.0",
    "@parcel/watcher": "2.5.1",
    "@tatsutakeinjp/eslint-config": "workspace:*",
    "@tatsutakeinjp/prettier-config": "workspace:*",
    "@tatsutakeinjp/tailwind-config": "workspace:*",
    "@tatsutakeinjp/tsconfig": "workspace:*",
    "@types/node": "22.14.0",
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.6",
    "dotenv-cli": "7.4.4",
    "eslint": "9.24.0",
    "framer-motion": "11.18.2",
    "jiti": "2.4.2",
    "prettier": "3.5.3",
    "sugar-high": "0.9.3",
    "tailwindcss": "3.4.17",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@tatsutakeinjp/eslint-config/base",
      "@tatsutakeinjp/eslint-config/nextjs",
      "@tatsutakeinjp/eslint-config/react"
    ]
  },
  "prettier": "@tatsutakeinjp/prettier-config"
}
